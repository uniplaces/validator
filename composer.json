{
    "name": "ebidtech/validator",
    "type": "library",
    "description": "A validator on top of Symfony validator",
    "keywords": ["validator", "php", "library", "symfony"],
    "homepage": "http://github.com/ebidtech/validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Ebidtech",
            "email": "developers@ebidtech.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.3",
        "symfony/validator": ">=2.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "squizlabs/php_codesniffer": "~1.5.0",
        "phpmd/phpmd": "~1.5.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-0": { "EBT\\Validator": "src/" }
    }
}
